    "def get_scientist_name(link):\n",
    "    response= requests.get(link)\n",
    "    if response.status_code == 200:\n",
    "    \n",
    "        soup = BeautifulSoup(response.text, 'html.parser')\n",
    "        name=soup.find('span',{'class': 'mw-page-title-main'}).text\n",
    "        if name:\n",
    "            return name\n",
    "        else:\n",
    "            return 'unknown'       "
